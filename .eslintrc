{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "plugins": ["prettier", "react"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
        }
    },
    "globals": {
    "__DEV__": false,
    "__PROD__": false,
    "$": false,
    "jQuery": false
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
        "linebreak-style": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}